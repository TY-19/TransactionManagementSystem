{
  "ConnectionStrings": {
    "Default": "YOUR_CONNECTION_STRING"
  },
  "TimeApi": {
    "BaseUrl": "https://timeapi.io/api/TimeZone"
  },
  "FreeIp": {
    "BaseUrl": "https://freeipapi.com/api/json"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "Fatal",
        "System": "Information"
      }
    }
  },
  "AllowedHosts": "*"
}
